paas_basic_report_IP=192.168.2.200,192.168.2.201
paas_application_IP=192.168.2.200,192.168.2.201
paas_deployer_IP=192.168.2.200,192.168.2.201
paas_runtime_IP=192.168.2.200,192.168.2.201
paas_monitor_IP=192.168.2.200,192.168.2.201
paas_appstore_IP=192.168.2.200,192.168.2.201

